run
-lc
<%= for mod <- @mods do %>
  <%= for dep <- mod.deps do %>
    --dep <%= dep %>
  <% end %>

  <%= if c = mod.c do %>
    <%= Sema.render_sema(c) %>
  <% end %>
  -M<%= mod.name %>=<%= mod.path %>
  <%= if c = mod.c do %>
    <%= for {file, opts} <- c.src do %>
      <%= if opts != [] do %>
        -cflags <%= for opt <- opts, do: opt %> --
      <% end %>
    <%= file %>
    <% end %>
  <% end %>
<% end %>
